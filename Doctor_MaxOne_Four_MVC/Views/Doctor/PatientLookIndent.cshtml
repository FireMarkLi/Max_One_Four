
@{
    Layout = Layout = "~/Views/Shared/GShow.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PatientLookIndent</title>
    <script src="~/Scripts/jquery-3.3.1.js"></script>
</head>
<body style="text-align: center;">
    <div id="div" style="background:#ADD8E6">

    </div>
</body>
</html>
<script>
    SHow();
    function SHow() {
          $.ajax({
              url: "http://localhost:52083/Doctor/IndentParticulars?Usersid=" +@ViewBag.Usersid+"&DingDanid=" + @ViewBag.DingDanid,
            type: "get",
            success: function (h) {
                $("#div").empty();
                $(h).each(function () {
                    $("#div").append(""
                        + "         <div><a onclick='look(" + this.reservationId + ")'> 订单号：" + this.paymentName + "</a></div >   "
                        + "     <br />                        "
                        + "     <hr />                        "
                        + "     <div>就诊医生：" + this.doctorName + "</div>         "
                        + "     <br />                        "
                        + "     <div>就诊医院：" + this.hospitalName + "</div>         "
                        + "     <br />                        "
                        + "     <div>患者姓名：" + this.patientName + "</div>         "
                        + "     <br />"
                        + "<img src='http://localhost:52083" + this.reservationImg + "' style='width:200px;height:200px' />"
                        + "     <br />"
                        + "<div>订单标题：" + this.paymentTitle+"</div>"
                    +"");
                })
            }
        })
    }
</script>
